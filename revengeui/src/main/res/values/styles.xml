<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.RevengeUI.Button" parent="Widget.AppCompat.Button">
        <item name="textAllCaps">false</item>
        <item name="android:background">@drawable/revengeui_btn_default_mtrl_shape</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:elevation">5dp</item>
        <item name="springEffect">true</item>
    </style>

    <style name="Widget.RevengeUI.SeekBar" parent="Widget.AppCompat.SeekBar">
        <item name="background">@null</item>
        <item name="android:background">@null</item>
        <item name="android:thumb">@drawable/revengeui_seekbar_thumb</item>
        <item name="android:splitTrack">false</item>
        <item name="splitTrack">false</item>
    </style>

    <style name="Widget.RevengeUI.SeekBar.NotAnimated" parent="Widget.RevengeUI.SeekBar">
        <item name="android:thumb">@drawable/revengeui_seekbar_thumb_unpressed</item>
    </style>

    <style name="Widget.RevengeUI.SeekBar.Discrete" parent="Widget.AppCompat.SeekBar.Discrete">
        <item name="background">@null</item>
        <item name="android:background">@null</item>
        <item name="android:thumb">@drawable/revengeui_seekbar_thumb</item>
        <item name="android:splitTrack">false</item>
        <item name="splitTrack">false</item>
    </style>
</resources>