            </aapt:attr>
        </path>
        <path
            android:pathData="M 0 -8 C 4.419 -8 8 -4.419 8 0 C 8 4.419 4.419 8 0 8 C -4.419 8 -8 4.419 -8 0 C -8 -4.419 -4.419 -8 0 -8 Z"
